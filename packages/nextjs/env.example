# Login: WalletConnect only (no injected wallets)
NEXT_PUBLIC_CONTRACT_ADDRESS=put_devloped_contract_address_here
NEXT_PUBLIC_NFT_ADDRESS=put_devloped_nft_address_here
NEXT_PUBLIC_CHAIN_ID=11155111
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=put_walletconnect_project_id_here
# Optional: RPC URL for Sepolia to improve WalletConnect reliability
NEXT_PUBLIC_RPC_URL=https://rpc.sepolia.org

# Optional: Relayer host-chain RPC URL (FHEVM-compatible). If set, Relayer SDK
# will use this instead of wallet default RPC.
NEXT_PUBLIC_RELAYER_RPC_URL=

# Optional: Relayer service URL (Zama maintained or custom). If set, it will
# override the default value in SepoliaConfig.
NEXT_PUBLIC_RELAYER_URL=
